<?xml version='1.0'?>

<robot name="new_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:include filename="$(find my_robot)/urdf/new_robot.gazebo" />




<gazebo reference="wheel_right_front">
    <material>Gazebo/Green</material>
</gazebo>
<gazebo reference="wheel_right_back">
    <material>Gazebo/Green</material>
</gazebo>
<gazebo reference="wheel_left_front">
    <material>Gazebo/Green</material>
</gazebo>
<gazebo reference="wheel_left_back">
    <material>Gazebo/Green</material>
</gazebo>
<gazebo reference="wheel_right_center">
    <material>Gazebo/Green</material>
</gazebo>
<gazebo reference="wheel_left_center">
    <material>Gazebo/Green</material>
</gazebo>

<gazebo reference="chassis">  
  <visual>  
    <material>  
      <ambient>0.8225 0.19225 0.19225 1.0</ambient>  
      <diffuse>0.50754 0.50754 0.50754 1.0</diffuse>  
      <specular>0.508273 0.508273 0.508273 1.0</specular>  
      <emissive>0.0 0.0 0.0 0.0</emissive>  
    </material>  
  </visual>  
</gazebo>
<gazebo reference="tower">  
  <visual>  
    <material>  
      <ambient>0.8225 0.19225 0.19225 1.0</ambient>  
      <diffuse>0.50754 0.50754 0.50754 1.0</diffuse>  
      <specular>0.508273 0.508273 0.508273 1.0</specular>  
      <emissive>0.0 0.0 0.0 0.0</emissive>  
    </material>  
  </visual>  
</gazebo>
<gazebo reference="cap">  
  <visual>  
    <material>  
      <ambient>0.8225 0.19225 0.19225 1.0</ambient>  
      <diffuse>0.50754 0.50754 0.50754 1.0</diffuse>  
      <specular>0.508273 0.508273 0.508273 1.0</specular>  
      <emissive>0.0 0.0 0.0 0.0</emissive>  
    </material>  
  </visual>  
</gazebo>


	<link name="robot_footprint"></link>

	<joint name="robot_footprint_joint" type="fixed">
		<origin xyz="0 0 0" rpy="0 0 0" />
		<parent link="robot_footprint"/>
		<child link="chassis" />
	</joint>


    <link name='chassis'>    

		<inertial>
			<mass value="8.0"/>
		  	<origin xyz="0 0 0" rpy=" 0 0 0"/>
		  	<inertia
		      ixx="0.16" ixy="0" ixz="0"
		      iyy="0.16" iyz="0"
		      izz="0.16"/>
		</inertial>

		<collision>
		  	<origin xyz="0 0 0" rpy=" 0 0 -1.5707"/>
		  	<geometry>
		    	<box size="0.6 0.6 .1"/>
		  	</geometry>
		</collision>

		<visual>
		  	<origin xyz="0 0 0" rpy=" 0 0 -1.5707"/>
		  	<geometry>
		    	<box size="0.5 0.5 .1"/>
			</geometry>
		</visual>

	</link>
	
	
	
 
	<link name='wheel_left_front'>
		
		<inertial>
		<mass value="0.3"/>
		<origin xyz="0.0 0 0" rpy=" 0 0 0"/>
		<inertia
		  ixx="0.1" ixy="0" ixz="0"
		  iyy="0.1" iyz="0"
		  izz="0.1"/>
		</inertial>

		<visual>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.05"/>
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.05"/>
			</geometry>
		</collision>

	</link>

      
	<link name='wheel_left_back'>
		
        <inertial>
			<mass value="0.3"/>
			<origin xyz="0.0 0 0" rpy=" 0 0 0"/>
			<inertia
			  ixx="0.1" ixy="0" ixz="0"
			  iyy="0.1" iyz="0"
			  izz="0.1"/>
		</inertial>

		<visual>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.05"/>
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.05"/>
			</geometry>
		</collision>
		
	</link>
      
	<link name='wheel_right_front'>
		
		<inertial>
			<mass value="0.3"/>
			<origin xyz="0.0 0 0" rpy=" 0 0 0"/>
			<inertia
			  ixx="0.1" ixy="0" ixz="0"
			  iyy="0.1" iyz="0"
			  izz="0.1"/>
		</inertial>

		<visual>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.05"/>
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.05"/>
			</geometry>
		</collision>
        
    </link>
      
      
      
      
	<link name='wheel_right_back'>
		
		<inertial>
			<mass value="0.3"/>
			<origin xyz="0.0 0 0" rpy=" 0 0 0"/>
			<inertia
			  ixx="0.1" ixy="0" ixz="0"
			  iyy="0.1" iyz="0"
			  izz="0.1"/>
		</inertial>

		<visual>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.05"/>
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.05"/>
			</geometry>
		</collision>
		
    </link>



    <link name='tower'>
		
		<inertial>
			<mass value="1"/>
			<origin xyz="0.0 0 0" rpy=" 0 0 0"/>
			<inertia
			  ixx="0.1" ixy="0" ixz="0"
			  iyy="0.1" iyz="0"
			  izz="0.1"/>
		</inertial>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.22" length="0.12"/>
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.22" length="0.12"/>
			</geometry>
		</collision>      

	</link>
      
      
      
	<joint name="chassis_tower" type="fixed">
		<origin xyz="0 0 0.10" rpy="0 0 0" />
		<parent link="chassis"/>
		<child link="tower" />
	</joint>      

	<joint name="left_front_axis" type="continuous">
		<origin xyz="0.2 0.278 0.02" rpy="0 0 0"/>
		<child link="wheel_left_front"/>
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<dynamics damping="1.0" friction="1.0"/>
	</joint>      

	<joint name="left_back_axis" type="continuous">
		<origin xyz="-0.2 0.278 0.02" rpy="0 0 0"/>
		<child link="wheel_left_back"/>
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<dynamics damping="1.0" friction="1.0"/>
	</joint>           

	<joint name="right_back_axis" type="continuous">
		<origin xyz="-0.2 -0.278 0.02" rpy="0 0 0"/>
		<child link="wheel_right_back"/>
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<dynamics damping="1.0" friction="1.0"/>
	</joint>  

	<joint name="right_front_axis" type="continuous">
		<origin xyz="0.2 -0.278 0.02" rpy="0 0 0"/>
		<child link="wheel_right_front"/>
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<dynamics damping="1.0" friction="1.0"/>
	</joint>       


	
	
	
	
	
	<link name="hokuyo">
		<inertial>
		<mass value="1e-5"/>
		<origin xyz="0.0 0 0" rpy=" 0 0 0"/>
		<inertia
			ixx="1e-6" ixy="0" ixz="0"
			iyy="1e-6" iyz="0"
			izz="1e-6"/> 
		</inertial>

		<collision>
		    <origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
			  <box size="0.1 0.1 0.1"/>
			</geometry> 
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
		    <geometry>  
		      <mesh filename="package://my_robot/meshes/hokuyo.dae"/>
		    </geometry>
		</visual>
	</link>
  
  <joint type="fixed" name="hokuyo_joint">
    <origin xyz="0 0 0.08" rpy="0 0 0"/>
    <child link="hokuyo"/>
    <parent link="tower"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>




	<link name="camera">
		<inertial>
			<mass value="1e-5"/>
			<origin xyz="0.0 0 0" rpy=" 0 0 0"/>
			<inertia
				ixx="1e-6" ixy="0" ixz="0"
				iyy="1e-6" iyz="0"
				izz="1e-6"/> 
		</inertial>

		<collision>
		    <origin xyz="-0.03 0 0.05" rpy="0 0 0" />
			<geometry>
			  <box size="0.1 0.1 0.1"/>
			</geometry> 
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 1.5707"/>
		    <geometry>  
		      <mesh filename="package://my_robot/meshes/old_camera.dae" scale="0.035 0.035 0.035"/>
		    </geometry>
		</visual>
	</link>
  
  <joint type="fixed" name="camera_joint">
    <origin xyz="0.265 0 -0.03" rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>


</robot>
      

